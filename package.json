{
    "name": "fvn",
    "displayName": "fvn",
    "description": "F is for friends",
    "version": "0.0.1",
    "repository": "https://github.com/0-5788719150923125/fvn",
    "engines": {
        "vscode": "^1.83.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./src/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "fvn.fire",
                "title": "fire"
            }
        ],
        "languages": [
            {
                "id": "src",
                "mimetypes": [
                    "text/x-code-output"
                ]
            }
        ],
        "grammars": [
            {
                "language": "src",
                "scopeName": "code.output",
                "path": "./syntaxes/src.tmGrammar.json"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js",
        "vscode:prepublish": "npm run build-base -- --minify",
        "build-base": "esbuild ./src/extension.js --bundle --outfile=build/main.js --external:vscode --format=cjs --platform=node",
        "build": "npm run build-base -- --sourcemap",
        "build-watch": "npm run build-base -- --sourcemap --watch",
        "test-compile": "tsc -p ./",
        "package": "npx vsce package"
    },
    "dependencies": {
        "gun": "https://github.com/amark/gun.git"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.2",
        "@types/node": "18.x",
        "@types/vscode": "^1.83.0",
        "@vscode/test-electron": "^2.3.4",
        "esbuild": "^0.19.5",
        "eslint": "^8.50.0",
        "glob": "^10.3.3",
        "mocha": "^10.2.0",
        "prettier": "^2.8.8",
        "typescript": "^5.2.2"
    }
}
