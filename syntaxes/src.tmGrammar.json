{
    "scopeName": "code.output",
    "patterns": [{ "include": "#expression" }],
    "repository": {
        "expression": {
            "patterns": [
                { "include": "#input" },
                { "include": "#output" },
                { "include": "#paren-expression" }
            ]
        },
        "input": {
            "match": "< ONE@CELL:",
            "name": "keyword.input"
        },
        "output": {
            "match": "> ONE@CELL:",
            "name": "keyword.output"
        },
        "paren-expression": {
            "begin": "\\(",
            "end": "\\)",
            "beginCaptures": {
                "0": { "name": "punctuation.paren.open" }
            },
            "endCaptures": {
                "0": { "name": "punctuation.paren.close" }
            },
            "name": "expression.group",
            "patterns": [{ "include": "#expression" }]
        }
    }
}
